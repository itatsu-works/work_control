<h1>仕事登録画面</h1>

<%= form_with model: @work, url: group_works_path, local: true do |f| %>

  <div class="work_name">
    <%= f.label :work_name %>
    <%= f.text_field :work_name, placeholder: 'Please input work name' %>
  </div>
  <div class="deadline">
    <%= f.label :deadline %>
    <%= f.date_field :deadline %>
  </div>
  <div class="contents">
    <%= f.label :contents %>
    <%= f.text_area :contents %>
  </div>
  
  

  <%= f.submit %>

<% end %>